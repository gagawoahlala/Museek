<body style="display:none;">
	<div class="container-fluid">


		{{#if viewAlt }}
			<nav class="navbar navbar-default navbar-fixed-top">
					<div class="navbar-header">
						<button style="visibility:hidden" type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
						</button>
						<span class="navbar-brand">Museek</span>
						<a href="/helper" class="glyphicon glyphicon-question-sign" aria-hidden="true"></a>
					</div>
			</nav>
			<main class="container-fluid">
				<div class="btn-group-vertical" role="group" aria-label="">
					<div id="chapters" class="viewAlt-button-group">
						{{#each chapters}}
							<div class="dropdown closed">
								<div id="chapter{{@index}}" class="title button-disabled">{{name}}</div>
								<ul>
									{{#each lessons}}
									<li class="title button-disabled" id="c{{chapterIndex}}l{{@index}}" onclick="location.href = '/quiz/?chapter={{chapterIndex}}&lesson={{@index}}';">{{name}}</li>
									{{/each}}
								</ul>
							</div>
						{{/each}}
						<!-- <button href="/?chapter=0">The Language of Music</button>
						<button class="button-disabled" href="/?chapter=1">Notes</button>
						<button class="button-disabled" href="/?chapter=2">Rhythm</button>
						<button class="button-disabled" href="/?chapter=3">Rests</button>
						<button class="button-disabled" href="/?chapter=4">Final</button> -->
					</div>
				</div>
			</main>
		{{else}}
			<nav class="navbar navbar-default navbar-fixed-top">
					<div class="navbar-header">
						<button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#bs-example-navbar-collapse-1" aria-expanded="false">
							<span class="sr-only">Toggle navigation</span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
							<span class="icon-bar"></span>
						</button>
						<span class="navbar-brand">{{name}}</span>
						<a href="/helper" class="glyphicon glyphicon-question-sign" aria-hidden="true"></a>
					</div>
					<div class="collapse navbar-collapse" id="bs-example-navbar-collapse-1">
						<div id="chapters" class="scrollmenu">
							<a href="/?chapter=0">The Language of Music</a>
							<a class="button-disabled" href="/?chapter=1">Notes</a>
							<a class="button-disabled" href="/?chapter=2">Rhythm</a>
							<a class="button-disabled" href="/?chapter=3">Rests</a>
							<a class="button-disabled" href="/?chapter=4">Final</a>
						</div>
					</div>
			</nav>
			<main class="container-fluid">
				<div class="btn-group-vertical" role="group" aria-label="">
					{{#each lessons}}
							<button id="c{{currentChapter}}l{{@index}}" onclick="location.href = '/quiz/?chapter={{currentChapter}}&lesson={{@index}}';"class="universal-button button-disabled">{{name}}</button>
					{{/each}}
				</div>
			</main>
		{{/if}}

		<nav class="navbar navbar-default navbar-fixed-bottom">
		  <div class="container navbar-footer">
				<a href="/stat" class="glyphicon glyphicon-stats" aria-hidden="true"></a>
				<a href="/profile" class="glyphicon glyphicon-user" aria-hidden="true"></a>
				<a href="/setting" class="glyphicon glyphicon-cog" aria-hidden="true"></a>
		  </div>
		</nav>
	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.min.js"></script>
	<script src="/js/index.js"></script>
	<script src="https://www.gstatic.com/firebasejs/4.10.1/firebase.js"></script>
	<script>
		var myStorage = window.localStorage;
		myStorage.setItem('currentChapterLength', {{lessons.length}});
		if (myStorage.getItem('viewAlt') == 'undefined' || myStorage.getItem('viewAlt') == null) {
			myStorage.setItem('viewAlt',true);
			if ("{{viewAlt}}" == "") {
				myStorage.setItem('viewAlt',false);
			}
		}
	  // Initialize Firebase

	</script>
